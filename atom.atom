---
layout: nil
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>{{ site.github.project_title }}</title>
  <link href="{{ site.github.url }}"/>
  <link type="application/atom+xml" rel="self" href="/{{ site.github.repository_name }}{{ page.url }}"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>{{ site.github.url }}</id>
  <author>
    <name>{{ site.github.owner_name }}</name>
    <email>you@example.com</email>
  </author>{% for post in site.posts %}
  <entry>
    <id>{{ site.github.url }}{{ post.id }}</id>
    <link type="text/html" rel="alternate" href="{{ site.github.url }}{{ post.url }}"/>
    <title>{{ post.title }}</title>
    <published>{{ post.date | date_to_xmlschema }}</published>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <author>
      <name>{{ site.github.owner_name }}</name>
      <uri>{{ site.github.url }}</uri>
    </author>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>{% endfor %}
</feed>
